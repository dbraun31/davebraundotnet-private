library(tidyverse)
library(RColorBrewer)
sev <- function(rew) {
return(b_j + rew*a_j)
}
per <- function(p) {
return(p)
}
cost <- function(pl_e) {
return(exp((pl_e * c_j)/e_j)-1)
}
V <- function(pl_e, pl_p, p, rew) {
return(-sev(rew) * per(p) * (1 - pl_p) - cost(pl_e))
}
softmax <- function(z) {
out <- c()
count <- 0
den <- sum(exp(z))
for (e in z) {
count <- count + 1
out[count] <- exp(e) / den
}
return(out)
}
